.sticky_container {
    position: sticky;
    top: 20px; 
    left: 20px; 
    z-index: 100; 
    background-color: rgba(142, 202, 230, 0.34); 
    padding: 10px;
    border-radius: 10px;
    width: 280px; 
    height: 150px;
    font-family: Montserrat;
    border: 5px solid white; 
    font-weight: bold;
    font-size: 20px;
  }
  

  